@model SiuntuPristatymas.Data.Models.Delivery

<div>
    <h4>Delivery</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Status)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FilledCapacity)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FilledCapacity)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Date)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Date)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.EstimatedDuration)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EstimatedDuration)
        </dd>

        <dt>
            Parcels
        </dt>
        <dd>
            <table class="table">
                <thead>
                    <tr>
                        <th>
                            Length
                        </th>
                        <th>
                            Width
                        </th>
                        <th>
                            Height
                        </th>
                        <th>
                            weight
                        </th>
                        <th>
                            Status
                        </th>

                        <th>
                            Address
                        </th>
                    </tr>
                </thead>
                @foreach (var item in Model.Parcels)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Length)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Width)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Height)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Weight)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Status)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.Address.Id)
                        </td>
                    </tr>
                }
            </table>
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
